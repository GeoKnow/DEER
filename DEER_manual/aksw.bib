  citeseerurl = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.161.4775},
  doi = {doi:10.1007/978-3-642-13489-0_21},
  keywords = {group_aksw event_eswc ontowiki martin unbehauen auer cache sys:relevantFor:infai
	sys:relevantFor:bis 2010 ontowiki peer-reviewed ontowiki_eu},
