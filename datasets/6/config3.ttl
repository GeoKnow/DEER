@prefix : <http://geoknow.org/specsontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

:d1		a				:Dataset ;


		:inputFile			"/home/abddatascienceadmin/deer/datasets/6/input.nt" .

:d2		a				:Dataset ;
		:outputFile		"/home/abddatascienceadmin/deer/datasets/6/output.ttl" ;
		:outputFormat	"Turtle" .

:geoL	        a			:Module, :GeoLocatorModule  ;
		rdfs:label		"Geo Locator Module" ;
		:hasInput		:d1 ;
		:hasOutput		:d2 ;
		:hasParameter	        :geoPram1, :geoPram2, :geoPram3.

:geoPram1	a		        :ModuleParameter, :GeoLocatorModuleParameter ;
		:hasKey			"inputaddressproperty" ;
		:hasValue		"http://deer.org/street" .

:geoPram2	a		       :ModuleParameter, :GeoLocatorModuleParameter ;
		:hasKey			"outputlatproperty" ;
		:hasValue		"http://deer.org/lat" .



