@prefix : <http://geoknow.org/specsontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbpedia-owl: <http://dbpedia.org/ontology/> .

:d1		a				:Dataset ;
		:inputFile		"/home/sherif/JavaProjects/GeoKnow/GeoLift/evaluations/pipeline_learner/drugbank/drugbank_dump.nt".
:d2		a				:Dataset .
:d3		a				:Dataset .
:d4		a				:Dataset ;
		:outputFile		"/home/sherif/JavaProjects/GeoKnow/GeoLift/evaluations/pipeline_learner/drugbank/drugbank_dump_manual_enrichmed.ttl" ;
		:outputFormat	"Turtle" .

:deref	a				:Module, :DereferencingModule  ;
		rdfs:label		"Dereferencing module" ;
		:hasInput		:d1 ;
		:hasOutput		:d2 ;
		:hasParameter	:derefParam1, :derefParam2 .

:derefParam1	a		:ModuleParameter, :DereferencingModuleParameter ;
		:hasKey			"inputProperty1" ;
		:hasValue		 dbpedia-owl:abstract .
		
:derefParam2	a		:ModuleParameter, :DereferencingModuleParameter ;
		:hasKey			"outputProperty1" ;
		:hasValue		dbpedia-owl:abstract .

:authorityConform	a	:Module, :AuthorityConformationModule  ;
		rdfs:label		"Authority Conformation module" ;
		:hasInput		:d2 ;
		:hasOutput		:d3 ;
		:hasParameter	:authorityConformPram1, :authorityConformPram2 .

:authorityConformPram1	a	:ModuleParameter, :AuthorityConformationModuleParameter ;
		:hasKey			"sourceSubjectAuthority" ;
		:hasValue		"http://dbpedia.org/resource" .

:authorityConformPram2	a	:ModuleParameter, :AuthorityConformationModuleParameter ;
		:hasKey			"targetSubjectAuthority" ;
		:hasValue		"http://wifo5-04.informatik.uni-mannheim.de/drugbank/resource/drugs" .

:authorityConform2	a	:Module, :AuthorityConformationModule  ;
		rdfs:label		"Authority Conformation module" ;
		:hasInput		:d3 ;
		:hasOutput		:d4 ;
		:hasParameter	:authorityConform2Pram1, :authorityConform2Pram2 .

:authorityConform2Pram1	a	:ModuleParameter, :AuthorityConformationModuleParameter ;
		:hasKey			"sourceSubjectAuthority" ;
		:hasValue		"http://wifo5-04.informatik.uni-mannheim.de/drugbank/resource/drugs" .

:authorityConform2Pram2	a	:ModuleParameter, :AuthorityConformationModuleParameter ;
		:hasKey			"targetSubjectAuthority" ;
		:hasValue		"http://deer.org/biomedical/resource" .



