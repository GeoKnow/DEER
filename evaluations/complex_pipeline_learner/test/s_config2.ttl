@prefix RDFS:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix DEER:  <http://geoknow.org/specsontology/> .

DEER:dataset_1  a       DEER:Dataset ;
        DEER:inputFile  "inputFile.ttl" .

DEER:parameter_13  a   DEER:AuthorityConformationModuleParameter , DEER:ModuleParameter ;
        DEER:hasKey    "targetSubjectAuthority" ;
        DEER:hasValue  "http://example.com/resource/" .

DEER:dataset_2  a        DEER:Dataset ;
        DEER:outputFile  "outputFile.ttl" .

DEER:parameter_14  a   DEER:AuthorityConformationModuleParameter , DEER:ModuleParameter ;
        DEER:hasKey    "sourceSubjectAuthority" ;
        DEER:hasValue  "http://dbpedia.org/resource" .

DEER:authority_conformation_module_5
        a                  DEER:AuthorityConformationModule , DEER:Module ;
        DEER:hasInput      DEER:dataset_1 ;
        DEER:hasOutput     DEER:dataset_2 ;
        DEER:hasParameter  DEER:parameter_14 , DEER:parameter_13 .
