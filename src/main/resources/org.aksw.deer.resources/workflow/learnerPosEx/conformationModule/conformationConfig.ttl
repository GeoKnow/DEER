		:outputFormat	"Turtle" .

:d2		a				:Dataset ;
		:outputFile		"/home/sherif/JavaProjects/GeoKnow/GeoLift/src/main/resources/org.aksw.deer.resources.workflow/conformationBerlin.ttl" ;
		:outputFormat	"Turtle" .

:conform	a				:Module, :ConformationModule  ;
